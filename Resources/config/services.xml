    <parameters>
        <parameter key="libphonenumber.phone_number_util.class">libphonenumber\PhoneNumberUtil</parameter>
    </parameters>

    <services>

        <service id="libphonenumber.phone_number_util" class="%libphonenumber.phone_number_util.class%"
